---
import Layout from "../layouts/Layout.astro";
import Cta from "../components/Shared/Cta.astro";
---

<Layout title="Documentation | Apptainer - Portable, Reproducible Containers">
	<div class="mt-20 bg-white px-6 py-24 sm:py-32 lg:px-8">
		<div class="mx-auto max-w-2xl text-center">
			<h2 class="text-4xl font-bold uppercase tracking-tight text-gray-900 sm:text-6xl">Singularity Documentation</h2>
		</div>
	</div>
	<div class="mx-auto mb-40 max-w-4xl">
		<div class="px-4 sm:px-6 lg:px-8">
